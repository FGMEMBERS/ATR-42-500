<!-- ATR-42-500 Flightdeck by Narendran M -->

<PropertyList>

 <path>Cockpit.ac</path>

<!-- Cockpit Illumination -->

	<animation>
		<name>Cockpit Illumination Select</name>
		<type>select</type>
		<object-name>object-lit</object-name>
		<condition>
			<property>controls/lighting/flood</property>
		</condition>
	</animation>

	<animation>
		<name>Cockpit illumination</name>
		<type>material</type>
		<object-name>object-lit</object-name>
		<emission>
			<red>0.13</red>
			<green>0.13</green>
			<blue>0.13</blue>
			<factor-prop>controls/lighting/flood</factor-prop>
		</emission>
	</animation>

<!-- Aircraft Crew - Pilot / Co-pilot -->

	<animation>
		<name>Show / Hide Pilot on Select Animation</name>
		<type>select</type>
		<object-name>pilot</object-name>
		<condition>
			<property>controls/lighting/pilot</property>
		</condition>
	</animation>

	<animation>
		<name>Show / Hide Co-Pilot on Select Animation</name>
		<type>select</type>
		<object-name>co-pilot</object-name>
		<condition>
			<property>controls/lighting/co-pilot</property>
		</condition>
	</animation>

