<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<path>flaps.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>Flapneedle</object-name>
        <property>surface-positions/flap-pos-norm</property>
        <factor>-30</factor>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0 </z>
        </axis>
    </animation>

</PropertyList>
