    <rumble>
        <name>rumble</name>
        <mode>looped</mode>
        <path>Sounds/rumble.wav</path>
        <condition>
            <or>
                <property>/gear/gear[0]/wow</property>
                <property>/gear/gear[1]/wow</property>
                <property>/gear/gear[2]/wow</property>
            </or>
        </condition>
        <volume>
            <property>/velocities/groundspeed-kt</property>
            <type>log</type>
            <factor>0.5</factor>
        </volume>
        <position>
            <x>-10</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>25.0</max-dist>
    </rumble>

    <gear1>
        <name>gear</name>
        <mode>in-transit</mode>
        <path>Aircraft/ATR-42-500/Sounds/gear.wav</path>
        <property>/gear/gear[0]/position-norm</property>
        <pitch>
            <internal>dt_play</internal>
            <offset>0.7</offset>
            <factor>-0.001</factor>
        </pitch>
        <position>
            <x>6</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </gear1>

    <gear1>
        <name>gear-lock</name>
        <path>Sounds/gear-lck.wav</path>
        <condition>
            <equals>
                <property>/gear/gear[0]/position-norm</property>
                <value>0</value>
            </equals>
        </condition>
        <position>
            <x>6.5</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>20.0</max-dist>
    </gear1>

<squeal0>
        <name>squeal</name>
        <path>Aircraft/ATR-42-500/Sounds/touchdown.wav</path>
        <condition>
            <or>
                <property>/gear/gear[0]/wow</property>
                <property>/gear/gear[1]/wow</property>
                <property>/gear/gear[2]/wow</property>
            </or>
        </condition>
        <volume>
            <property>sim/sound/Ovolume</property>
        </volume>
        <volume>
            <property>/velocities/speed-down-fps</property>
            <factor>0.05</factor>
        </volume>
        <volume>
            <property>/velocities/airspeed-kt</property>
            <factor>0.01</factor>
        </volume>
        <volume>
            <internal>dt_stop</internal>
            <factor>0.05</factor>
            <max>1.0</max>
        </volume>
        <pitch>
            <property>/velocities/airspeed-kt</property>
            <factor>0.0025</factor>
            <offset>1.2</offset>
        </pitch>
        <position>
            <x>-6.5</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>20.0</reference-dist>
        <max-dist>40.0</max-dist>
    </squeal0>


    <squeal1>
      <name>squeal</name>
      <path>Aircraft/ATR-42-500/Sounds/touch.wav</path>
      <condition>
	<not>
            <less-than>
                <property>velocities/groundspeed-kt</property>
                <value>45</value>
            </less-than>
	</not>
      </condition>
        <volume>
                <property>sim/sound/Evolume</property>
        </volume>
        <position>
            <x>0</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </squeal1>

    <flap>
        <name>flaps</name>
        <mode>in-transit</mode>
        <path>Aircraft/ATR-42-500/Sounds/Flaps.wav</path>
        <property>/surface-positions/flap-pos-norm</property>
        <volume>
            <factor>2.25</factor>
